{
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/expect": "jsr:@std/expect@1",
    "@std/dotenv/load": "jsr:@std/dotenv@0.225.6/load",
    "@std/http/file-server": "jsr:@std/http@1/file-server",
    "@std/path": "jsr:@std/path@1",
    "@std/testing/bdd": "jsr:@std/testing@1/bdd",
    "@fresh/core": "jsr:@fresh/core@2.2.0",
    "@fresh/core/dev": "jsr:@fresh/core@2.2.0/dev",
    "@fresh/core/render": "jsr:@fresh/core@2.2.0/render",
    "@fresh/core/runtime": "jsr:@fresh/core@2.2.0/runtime",
    "@preact/signals": "npm:@preact/signals@2.5.1",
    "@preact/signals-core": "npm:@preact/signals-core@1.12.1",
    "react": "npm:preact@10.28.2/compat",
    "react-dom": "npm:preact@10.28.2/compat",
    "react-dom/client": "npm:preact@10.28.2/compat",
    "react-dom/server": "npm:preact@10.28.2/compat",
    "react/jsx-runtime": "npm:preact@10.28.2/jsx-runtime",
    "react/jsx-dev-runtime": "npm:preact@10.28.2/jsx-runtime",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/",
    "@twind/preset-autoprefix": "https://esm.sh/@twind/preset-autoprefix@1.0.7",
    "@twind/preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4",
    "@ai-sdk/openai": "npm:@ai-sdk/openai@1.3.22",
    "@mastra/core": "npm:@mastra/core@0.24.9",
    "@mastra/core/agent": "npm:@mastra/core@0.24.9/agent",
    "@mastra/core/scores": "npm:@mastra/core@0.24.9/scores",
    "@mastra/evals": "npm:@mastra/evals@0.14.4",
    "@mastra/evals/judge": "npm:@mastra/evals@0.14.4/judge",
    "@mastra/evals/nlp": "npm:@mastra/evals@0.14.4/nlp",
    "ai": "npm:ai@4.3.16",
    "p-limit": "npm:p-limit@7.2.0",
    "zod": "npm:zod@4.3.5"
  },
  "tasks": {
    "dev": "deno run -A scripts/dev.ts",
    "test": "deno test -A --parallel",
    "test:config": "deno test -A tests/config.test.ts tests/connectivity.test.ts tests/deployment.test.ts",
    "test:connectivity": "CONNECTIVITY_TEST=true deno test -A tests/connectivity.test.ts",
    "pre-commit": "deno run -A scripts/pre-commit-checks.ts",
    "hooks:install": "deno run -A scripts/install-hooks.ts",
    "test:unit": "deno test -A tests/**/*.test.ts --ignore=tests/api",
    "test:api": "deno test -A tests/api/**/*.test.ts",
    "test:coverage": "deno test -A --coverage=coverage",
    "test:e2e": "deno run -A --node-modules-dir npm:@playwright/test@1.57.0 test",
    "test:e2e:install": "deno run -A --node-modules-dir npm:@playwright/test@1.57.0 install",
    "test-server": "deno run -A scripts/test-server.ts",
    "typecheck": "deno check deploy/main.ts src/server/main.ts src/cli/optimize.ts src/cli/generate.ts src/cli/eval-dist.ts",
    "optimize": "deno run -A src/cli/optimize.ts",
    "generate": "deno run -A src/cli/generate.ts",
    "eval:dist": "deno run -A src/cli/eval-dist.ts",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "fmt:check": "deno fmt --check",
    "ui:build": "deno run -A scripts/ui-build.ts",
    "ui:dev": "cd src/ui && deno run -A --watch=static/,routes/,islands/ main.ts",
    "ui:start": "cd src/ui && deno run -A main.ts"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact@10.28.2"
  },
  "nodeModulesDir": "auto",
  "deploy": {
    "project": "f75fcfab-6b0c-458c-ae47-e69714ace613",
    "exclude": [
      "**/node_modules"
    ],
    "include": [
      "deploy",
      "src",
      "data",
      "prompts",
      "src/ui",
      "deno.json",
      "deno.lock"
    ],
    "entrypoint": "deploy/main.ts"
  }
}
